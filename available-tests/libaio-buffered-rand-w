[global]
description=4k/64k/512k blocks (Buffered I/O and Sync on Finish)
rw=randwrite
direct=0
size=${TEST_SIZE}
ioengine=libaio
iodepth=1
filename=${TEST_FILENAME}
directory=${TEST_DIRECTORY}

[libaio-buffered-sync-4k-r]
bs=4k

[libaio-buffered-sync-64k-r]
bs=64k

[libaio-buffered-sync-512k-r]
bs=512k
